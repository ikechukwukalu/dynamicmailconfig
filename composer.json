{
    "name": "ikechukwukalu/dynamicmailconfig",
    "description": "A laravel package that enables each user to send emails through your platform, using their own unique email configuration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ikechukwukalu\\Dynamicmailconfig\\": "src/",
            "Ikechukwukalu\\Dynamicmailconfig\\Middleware\\": "src/Middleware/",
            "Ikechukwukalu\\Dynamicmailconfig\\Models\\": "src/Models/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ikechukwukalu\\Dynamicmailconfig\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Ikechukwu Kalu",
            "email": "ea.ikechukwukalu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "php-parallel-lint/php-parallel-lint": "dev-develop"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikechukwukalu\\Dynamicmailconfig\\DynamicMailConfigServiceProvider"
            ]
        }
    }
}
